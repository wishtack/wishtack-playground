{
  "name": "demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "demo",
  "sourceRoot": "apps/demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/demo",
        "index": "{projectRoot}/src/index.html",
        "browser": "{projectRoot}/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "{projectRoot}/tsconfig.app.json",
        "assets": [
          "{projectRoot}/src/favicon.ico",
          "{projectRoot}/src/assets"
        ],
        "styles": [
          "{projectRoot}/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "demo:build:production"
        },
        "development": {
          "buildTarget": "demo:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "demo:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "angular-cli-jest": {
      "executor": "@angular-devkit/build-angular:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "include": [
          "{projectRoot}/**/*.spec.ts"
        ],
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "{projectRoot}/tsconfig.spec.json"
      }
    },
    "angular-cli-karma": {
      "executor": "@angular-devkit/build-angular:karma",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "browsers": "ChromeHeadless",
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "{projectRoot}/tsconfig.spec.json",
        "watch": false
      }
    },
    "angular-cli-web-test-runner": {
      "executor": "@angular-devkit/build-angular:web-test-runner",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "include": [
          "{projectRoot}/**/*.spec.ts"
        ],
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "{projectRoot}/tsconfig.spec.json"
      }
    },
    "jest": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.ts"
      }
    },
    "jest-swc": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "{projectRoot}/jest-swc.config.ts"
      }
    },
    "vitest": {
      "_comment": "override analog's override which is setting the pool to vmThreads",
      "command": "vitest --config {projectRoot}/vitest.config.ts --pool threads --watch false"
    },
    "vitest-no-isolate": {
      "_comment": "override analog's override which is setting the pool to vmThreads",
      "command": "vitest --config {projectRoot}/vitest.config.ts --pool threads --no-isolate --watch false"
    },
    "vitest-swc": {
      "command": "vitest --config {projectRoot}/vitest-swc.config.ts --watch false"
    },
    "vitest-swc-no-isolate": {
      "command": "vitest --config {projectRoot}/vitest-swc.config.ts --no-isolate --watch false"
    }
  }
}
