{
  "name": "demo-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "tests/demo-e2e/src",
  "implicitDependencies": ["demo"],
  "// targets": "to see all targets run: nx show project demo-e2e --web",
  "targets": {
    "report-c8": {
      "command": "c8 report -r html -r text --temp-directory dist/coverage-tmp"
    },
    "report-istanbul": {
      "command": "node --experimental-strip-types tools/report-istanbul.mts"
    },
    "report-mcr": {
      "command": "node --experimental-strip-types tools/report-mcr.mts"
    }
  }
}
