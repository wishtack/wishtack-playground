{
  "scripts": {
    "build": "rm -Rf dist && tsc -p tsconfig.app.json && cp src/*.openapi.yaml dist",
    "codegen": "tools/generate-dtos.sh",
    "deploy": "tools/deploy.sh",
    "start": "nodemon src/main.ts --watch src --ext ts,yaml",
    "test-narrow": "vitest",
    "test-wide": "playwright test --reporter html"
  },
  "dependencies": {
    "axios": "1.2.1",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-openapi-validator": "4.13.5",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.1.4",
    "memoizee": "0.4.15"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@playwright/test": "1.47.0",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/memoizee": "0.4.8",
    "@types/supertest": "2.0.12",
    "@yarnpkg/sdks": "3.0.0-rc.33",
    "nodemon": "2.0.19",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.6.2",
    "vite": "5.4.4",
    "vitest": "2.0.5"
  },
  "wallaby": {
    "tests": [
      "src/**/*.spec.ts"
    ]
  }
}
